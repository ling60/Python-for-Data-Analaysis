	file = {:D$\backslash$:/TonidoSync/work/Papers/References{\_}Papers/social{\_}media{\_}marketing/Online engagement factors on Facebook brand pages.pdf:pdf},
	isbn = {1869-5450},
	issn = {18695469},
	journal = {Social Network Analysis and Mining},
	keywords = {Facebook,Interaction,Online engagement,Social media marketing,Social networks,marketing,social media,social media and marketing,uses and gratifications},
	mendeley-groups = {why{\_}follow,Social Media and Marketing},
	mendeley-tags = {marketing,social media,social media and marketing,uses and gratifications},
	number = {4},
	pages = {843--861},
	pmid = {25246403},
	title = {{Online engagement factors on Facebook brand pages}},
	volume = {3},
